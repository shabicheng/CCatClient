.PHONY: clean All

All:
	@echo "----------Building project:[ CCatClientTest - Release ]----------"
	@cd "CodeLite/CCatClientTest" && "$(MAKE)" -f  "CCatClientTest.mk"
clean:
	@echo "----------Cleaning project:[ CCatClientTest - Release ]----------"
	@cd "CodeLite/CCatClientTest" && "$(MAKE)" -f  "CCatClientTest.mk" clean
